import machine
import utime

led = machine.Pin("LED", machine.Pin.OUT)

print("Function started.")
led.off()
utime.sleep(1)
led.on()
utime.sleep(1)
led.off()
utime.sleep(1)
led.on()
utime.sleep(1)
led.off()
utime.sleep(1)
led.on()
utime.sleep(1)
led.off()
utime.sleep(0.5)
led.on()
utime.sleep(0.5)
led.off()
utime.sleep(0.5)
led.on()
utime.sleep(0.5)
led.off()
utime.sleep(0.5)
led.on()
utime.sleep(0.5)
led.off()
utime.sleep(0.5)
led.on()
utime.sleep(0.5)
led.off()
utime.sleep(0.1)
led.on()
utime.sleep(0.1)
led.off()
utime.sleep(0.1)
led.on()
utime.sleep(0.1)
led.off()
utime.sleep(0.1)
led.on()
utime.sleep(0.1)
led.off()
utime.sleep(0.1)
led.on()
utime.sleep(0.1)
led.off()
utime.sleep(0.1)
led.on()
utime.sleep(0.1)
led.off()
utime.sleep(0.1)
led.on()
utime.sleep(0.1)
led.off()
utime.sleep(0.1)
led.on()
utime.sleep(0.1)
led.off()
utime.sleep(0.1)
led.on()
utime.sleep(0.1)
led.off()
print("Function completed.")
